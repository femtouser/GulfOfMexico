{
  "manifest_version": 3,
  "name": "Gulf Name Corrector",
  "version": "1.0",
  "description": "Replaces 'Gulf of America' with 'Gulf of Mexico' on web pages.",
  "permissions": ["activeTab", "scripting"],
  //"action": {
  //  "default_popup": "popup.html"
  //},
  //"background": {
  //  "service_worker": "background.js"
  //},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}